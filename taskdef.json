{
  "family": "shippable",
  "containerDefinitions": [
    {
      "name": "node-app",
      "image": "${APP_IMAGE_SOURCENAME}/${APP_IMAGE_VERSIONNAME}",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "memory": 100,
      "essential": true,
      "environment": [
        {
          "name": "ENVIRONMENT",
          "value": "cli"
        }
      ]
    }
  ]
}
